<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/bootstrap.css' />
    <style type="text/css">
        body {
            padding-top: 60px;
            padding-bottom: 40px;
        }
    </style>
    <link href="stylesheets/bootstrap-responsive.css" rel="stylesheet">
</head>

<script type="text/javascript">
    function show(){
        var array = "<%= array %>";
        var array2 = [];
        for (var i = 0; i < array.length; i++)	{
            if(!(i % 2)){
                array2.push(array[i]);
            }
        }
        for ( i = 0; i < array2.length; i++)	{
            document.form_version.select_version.options[i] = new Option(array2[i],i);
        }
    }
</script>
<body>

<div class="navbar navbar-fixed-top">
    <div class="navbar-inner">
        <div class="container">
            <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </a>
            <a class="brand" href="/">tools</a>
            <div class="nav-collapse">
                <ul class="nav">
                    <li class="active"><a href="/">首页</a></li>
                    <li><a href="/login">登入</a></li>
                    <li><a href="/logout">注销</a></li>
                </ul>
            </div>
        </div>
    </div>
</div>

<table width="280" height="160" border="0" align="center" cellpadding="5" cellspacing="0">
    <tr>
        <td height="34" ><span class="style1" style="font-weight:bold ">version</span></td>
    </tr>
    <form name="form_version">
        <select name="select_version" id="select_version" onFocus="show()">
        </select>
    </form>
</table>

<div id="container" class="container">
    <hr />
    <footer>
        <p><a href="http://blog.chinaunix.net/uid/8625039.html" target="_blank">King Lee</a> 2014</p>
    </footer>
</div>
</body>
<script src="/javascripts/jquery.js"></script>
<script src="/javascripts/bootstrap.js"></script>
</html>
